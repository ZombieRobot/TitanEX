<!--Titan-->
<window id="113">
	<defaultcontrol always="true">901</defaultcontrol>
	<allowoverlay>yes</allowoverlay>

	<controls>
	
		 <control type="group">
            <posx>740</posx>
            <posy>0</posy>
            <animation type="WindowOpen" reversible="false">
                <effect type="fade" start="0" end="100" time="0" tween="sine" easing="in"/>
                <effect type="slide" start="0,-75" end="0,0" time="0" tween="cubic" easing="out"/>
            </animation>
            <animation type="WindowClose" reversible="false">
                <effect type="fade" start="100" end="0" time="0" tween="sine" easing="out"/>
                <effect type="slide" start="0,0" end="0,-75" time="0" tween="sine" easing="out"/>
                <effect type="slide" end="0,-75" time="0" delay="300"/>
            </animation>          
            <control type="group">
				 <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>433</width>
                    <height>238</height>
                    <texture>overlays/volume/volume_panel.png</texture>
                </control>
                <control type="image">
                    <posx>230</posx>
                    <posy>43</posy>
                    <width>150</width>
                    <height>150</height>
                    <texture>overlays/volume/muted.png</texture>
                </control>                
                <control type="label">                    
                    <posx>85</posx>
                    <posy>70</posy>
                    <width>250</width>
                    <height>100</height>
                    <label>$INFO[Player.Volume]</label>
                    <font>TitanLight45</font>
                    <textcolor>TitanGrey</textcolor>
                    <scroll>false</scroll>
                    <align>left</align>
                </control>
            </control>
        </control>

	</controls>

</window>
